{
  "name": "@rlanz/bull-queue",
  "description": "Queue system based on BullMQ for AdonisJS",
  "version": "3.1.0",
  "engines": {
    "node": ">=18.16.0"
  },
  "main": "./build/index.js",
  "type": "module",
  "files": [
    "src",
    "build/commands",
    "build/providers",
    "build/services",
    "build/src",
    "build/stubs",
    "build/index.js",
    "build/index.d.ts",
    "build/configure.js",
    "build/configure.d.ts"
  ],
  "exports": {
    ".": "./build/index.js",
    "./commands": "./build/commands/main.js",
    "./commands/*": "./build/commands/*.js",
    "./services/*": "./build/services/*.js",
    "./queue_provider": "./build/providers/queue_provider.js"
  },
  "scripts": {
    "index:commands": "adonis-kit index build/commands",
    "build": "npm run lint && npm run clean && npm run build-only && npm run copyfiles && npm run index:commands",
    "build-only": "tsc",
    "clean": "del-cli build",
    "copyfiles": "copyfiles \"stubs/**/**/*.stub\" build",
    "format": "prettier --write .",
    "lint": "eslint .",
    "release": "npx release-it",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "@adonisjs/assembler": "^7.8.2",
    "@adonisjs/core": "^6.17.2",
    "@adonisjs/eslint-config": "^2.0.0",
    "@adonisjs/prettier-config": "^1.4.0",
    "@adonisjs/tsconfig": "^1.4.0",
    "@swc/core": "^1.11.1",
    "copyfiles": "^2.4.1",
    "del-cli": "^6.0.0",
    "eslint": "^9.21.0",
    "prettier": "^3.5.2",
    "release-it": "^18.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3"
  },
  "dependencies": {
    "@poppinss/utils": "^6.9.2",
    "@sindresorhus/is": "^7.0.1",
    "bullmq": "^5.41.6"
  },
  "peerDependencies": {
    "@adonisjs/core": "^6.5.0"
  },
  "author": "Romain Lanz <romain.lanz@pm.me>",
  "license": "MIT",
  "homepage": "https://github.com/romainlanz/adonis-bull-queue#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/romainlanz/adonis-bull-queue.git"
  },
  "bugs": {
    "url": "https://github.com/romainlanz/adonis-bull-queue/issues"
  },
  "keywords": [
    "adonisjs",
    "bullmq",
    "queue"
  ],
  "eslintConfig": {
    "extends": "@adonisjs/eslint-config/package"
  },
  "release-it": {
    "git": {
      "commitMessage": "chore(release): ${version}",
      "tagAnnotation": "v${version}",
      "tagName": "v${version}"
    },
    "github": {
      "release": true,
      "releaseName": "v${version}",
      "web": true
    }
  },
  "prettier": "@adonisjs/prettier-config",
  "publishConfig": {
    "tag": "latest",
    "access": "public"
  }
}
